@model IEnumerable<KnowledgePool.Models.Set>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BaseSetSize)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Block)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CardsphereSetId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsFoilOnly)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsForeignOnly)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsNonFoilOnly)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsOnlineOnly)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsPartialPreview)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KeyruneCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Languages)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.McmId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.McmIdExtras)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.McmName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MtgoCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ParentCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReleaseDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TcgplayerGroupId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TokenSetCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalSetSize)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Type)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BaseSetSize)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Block)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CardsphereSetId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsFoilOnly)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsForeignOnly)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsNonFoilOnly)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsOnlineOnly)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsPartialPreview)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KeyruneCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Languages)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.McmId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.McmIdExtras)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.McmName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MtgoCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ParentCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReleaseDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TcgplayerGroupId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TokenSetCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalSetSize)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Code">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Code">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Code">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
